</br>
<h3>Create New Code</h3>
<div class="row jumbotron">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for Code.new do |f| %>

      <%= f.label :"Log In Code" %>
      <%= f.text_field :logInCode, class: 'form-control' %>

      <%= f.label :"Search Option" %>
      <%= f.text_field :searchOption, class: 'form-control' %>

      <%= f.label :"Query Suggestion" %>
      <%= f.text_field :querySuggestion, class: 'form-control' %>

      </br>
      <%= f.submit "Generate", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
